class IsMobileService {
    constructor() { }
    getIsMobile() {
        var ua = navigator.userAgent;
        console.log("UA", ua);
        var isMobile = false;
        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini|Mobile|mobile|CriOS/i.test(ua)) {
            isMobile = true;
        }
        else if (/Chrome/i.test(ua)) {
            isMobile = false;
        }
        else {
            isMobile = false;
        }
        return isMobile;
    }
}
