      }, {
        key: "getRandomHeadLine",
        value: function getRandomHeadLine(text) {
          //console.log("text",text);
          text += ".";
          var exploded = text.split("\n");
          var index = Math.floor(Math.random() * (exploded.length - 1)); //console.log("index",index);

          if (index < 0 || index >= exploded.length) return "NO HEADLINE";
          return exploded[index];
        }
